<!DOCTYPE html>
<html lang="fr" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Liste de prestataires</title>
  </head>

  <body>
    <% list.forEach(function (prest) { %>
    <p>
      name: <%= prest.nom %><br>
      prenom: <%= prest.prenom %><br>
      <a href="/prest_details/<%= prest._id %>"> ID: <%= prest.id %></a><br>
    </p>
    <hr>
    <% }); %>
  </body>
</html>
